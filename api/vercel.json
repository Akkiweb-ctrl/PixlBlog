{
    "version":2,
    "builds":[
        {"src":"*.js","use":"@vercel/node"}
    ],
    "rewrites":[
        {
            "src":"/(.*)",
            "dest":"/"
        }
    ],
    "headers": [
        
        {
           "src":"/(.*)",
           "headers":[
            {
                "key":"Access-Control-Allow-Origin",
                "value":"*"
            }
           ]
        },
        {
            "src":"https://pixl-blog-one.vercel.app/(.*)",
            "headers":[
                {
                    "key":"Access-Control-Allow-Origin",
                    "value":"*"
                }
            ]   
        }
    ]
}